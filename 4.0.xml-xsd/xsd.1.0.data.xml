<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    On root: "xmlns" default namespace
    which provides further information to the Schema 
    from XML Instaces (XML document)

    On root: "xmlns:i" Schema namespace using valid URL

    Schema located on: http://localhost:8000/xml-xsd

    i:schemaLocation="http://localhost:8000/xml-xsd schema-file-path-goes-here"        
-->
<note xmlns="http://localhost:8000/xml-xsd"
    xmlns:i="http://www.w3.org/2001/XMLSchema-instance"
    i:schemaLocation="http://localhost:8000/xml-xsd xsd.1.1.data_schema.xsd"        
>
    <to>Rai</to>
    <from>Tom</from>
    <heading>Reminder</heading>
    <body>Don't forget me this weekend!</body>
</note>